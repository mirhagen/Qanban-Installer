<shortcuts>
  <skipIfNotSupported/>
  <programGroup defaultName="Qanban"
                location="applications||startMenu"/>
  <shortcut
    name="Qanban"
    target="$INSTALL_PATH\startQanban.bat $http.port $memory"
    commandLine=""
    workingDirectory="$INSTALL_PATH\"
    description="This starts Qanban"
	iconFile="$INSTALL_PATH\icons\start_qanban.png"
    iconIndex="0"
    initialState="normal"
    programGroup="yes"
    desktop="yes"
    applications="no"
    startMenu="no"
    startup="no"
	terminal="true"
	encoding="UTF-8">

  </shortcut>

  <shortcut
    name="Uninstall Qanban"
    target="$JAVA_HOME\bin\java"
    commandLine="-jar $INSTALL_PATH\Uninstaller\uninstaller.jar"
    workingDirectory="$INSTALL_PATH\"
    description="Uninstall Qanban"
	iconFile="$INSTALL_PATH\icons\uninstall.png"
    iconIndex="0"
    initialState="normal"
    programGroup="yes"
    desktop="no"
    applications="no"
    startMenu="no"
    startup="no"
	terminal="false"
	encoding="UTF-8">

  </shortcut>

</shortcuts>
